{
  "tasks": {
    "db:generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "licenses:check": {
      "inputs": ["package.json", "pnpm-lock.yaml"],
      "outputs": [
        "src/app/\\(userland\\)/\\(contained\\)/licenses/licenses.json"
      ]
    },
    "build": {
      "dependsOn": [
        "server:build",
        "db:generate",
        "licenses:check",
        "android:fetch"
      ],
      "inputs": ["**/*", "!dist/**", "!node_modules/**", "!public/workers/**"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": ["NEXT_PUBLIC_SUPABASE_URL", "NEXT_PUBLIC_SUPABASE_ANON_KEY"]
    },
    "dev": {
      "dependsOn": ["db:generate", "licenses:check"],
      "cache": false
    },
    "workers:dev": {
      "inputs": ["workers/**"],
      "outputs": ["public/workers/**"]
    },
    "workers:build": {
      "inputs": ["workers/**"],
      "outputs": ["public/workers/**"]
    },
    "server:dev": {
      "dependsOn": ["db:generate"],
      "inputs": ["server/**"],
      "outputs": ["dist/**"]
    },
    "server:build": {
      "dependsOn": ["db:generate"],
      "inputs": ["server/**"],
      "outputs": ["dist/**"]
    },
    "start": {
      "dependsOn": ["build", "db:generate"],
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "type-check": {
      "dependsOn": ["db:generate", "licenses:check"],
      "inputs": [
        "**/*.{ts,tsx,mdx}",
        "!dist/**",
        "!node_modules/**",
        "!public/workers/**",
        "!src/app/\\(userland\\)/\\(contained\\)/licenses/licenses.json"
      ]
    },
    "format": {
      "cache": false
    },
    "test": {
      "dependsOn": ["db:generate", "licenses:check"],
      "inputs": ["**/*", "!dist/**", "!node_modules/**", "!public/workers/**"]
    },
    "android:fetch": {
      "cache": false
    }
  }
}
