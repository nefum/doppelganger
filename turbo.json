{
  "tasks": {
    "web#db:generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "web#licenses:check": {
      "inputs": ["package.json", "pnpm-lock.yaml"],
      "outputs": [
        "src/app/\\(userland\\)/\\(contained\\)/licenses/licenses.json"
      ]
    },
    "build": {
      "dependsOn": [
        "web#server:build",
        "web#db:generate",
        "web#android:fetch",
        "web#licenses:check",
        "^build"
      ],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": ["NEXT_PUBLIC_SUPABASE_URL", "NEXT_PUBLIC_SUPABASE_ANON_KEY"]
    },
    "dev": {
      "dependsOn": [
        "web#db:generate",
        "web#licenses:check",
        "web#workers:dev",
        "^dev"
      ],
      "cache": false
    },
    "web#workers:dev": {
      "inputs": ["workers/**"],
      "outputs": ["public/workers/**"]
    },
    "web#workers:build": {
      "inputs": ["workers/**"],
      "outputs": ["public/workers/**"]
    },
    "web#server:dev": {
      "dependsOn": ["db:generate"],
      "inputs": ["server/**"],
      "outputs": ["dist/**"]
    },
    "web#server:build": {
      "dependsOn": ["db:generate"],
      "inputs": ["server/**"],
      "outputs": ["dist/**"]
    },
    "web#start": {
      "dependsOn": ["build", "db:generate"],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "type-check": {
      "dependsOn": ["web#db:generate", "web#licenses:check", "^type-check"]
    },
    "web#format": {
      "cache": false
    },
    "test": {
      "dependsOn": ["web#db:generate", "web#licenses:check", "^test"]
    },
    "web#android:fetch": {
      "cache": false
    }
  }
}
