{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "init": "git submodule update --init --recursive",
    "android:fetch": "bash scripts/download-scrcpy-server.sh & bash scripts/download-fdroid.sh",
    "db:generate": "prisma generate",
    "db:migrate": "dotenvx run --env-file=.env.local -- prisma migrate dev",
    "db:seed": "dotenvx run --env-file=.env.local -- tsx prisma/seed.ts",
    "server:build": "pkgroll --minify --src server/src --tsconfig=server/tsconfig.json --clean-dist --sourcemap && sentry-cli releases --project doppelganger-server new ${SENTRY_RELEASE:-$(git rev-parse HEAD)} && sentry-cli sourcemaps inject dist && sentry-cli sourcemaps --project doppelganger-server upload dist && sentry-cli releases --project doppelganger-server finalize ${SENTRY_RELEASE:-$(git rev-parse HEAD)}",
    "android:dev": "bash scripts/android-dev.sh",
    "dev": "BLUEBIRD_DEBUG=1 tsx server/src/index.ts",
    "workers:dev": "rimraf -rf public/workers/ && webpack --config workers/webpack.config.cjs",
    "workers:build": "rimraf -rf public/workers/ && webpack --config workers/webpack.config.cjs --mode production",
    "licenses:check": "rimraf \"src/app/(userland)/(contained)/licenses/licenses.json\" && license-checker --production --json --out \"src/app/(userland)/(contained)/licenses/licenses.json\"",
    "build": "next build",
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "lint": "next lint --max-warnings 0",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "ui:add": "pnpx shadcn-ui@latest add"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    },
    "./instrument": {
      "import": "./dist/instrument.mjs"
    }
  },
  "engines": {
    "node": ">=22.4.0"
  },
  "packageManager": "pnpm@8.15.5",
