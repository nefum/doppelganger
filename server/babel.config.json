{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "rewriteImportExtensions": true,
        "onlyRemoveTypeImports": true
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "packagejson",
        "root": ".",
        "alias": {
          "^@/(.+)": "./src/\\1",
          "^%/(.+)": "./server/src/\\1"
        }
      }
    ],
    "./babel-plugin-add-js-extension.cjs"
  ],
  "sourceMaps": true
}
