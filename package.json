{
  "name": "doppelganger-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "init": "git submodule update --init --recursive",
    "db:generate": "prisma generate",
    "db:migrate": "dotenvx run --env-file=.env.local -- prisma migrate dev",
    "db:seed": "dotenvx run --env-file=.env.local -- tsx prisma/seed.ts",
    "build-server": "rimraf -rf dist/ && babel --config-file \"./server/babel.config.server.json\" -x \".ts\" server -d dist",
    "dev": "nodemon -x \"turbo build-server && node dist/server.js\"",
    "build": "turbo build-server && next build",
    "start": "NODE_ENV=production node dist/server.js",
    "lint": "next lint --max-warnings 0",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "packageManager": "pnpm@8.15.5",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.5.0",
    "@next/third-parties": "^14.2.4",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "5.16.1",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@supabase/ssr": "^0.4.0",
    "@supabase/supabase-js": "^2.44.2",
    "async-lock": "^1.4.1",
    "bufferutil": "^4.0.8",
    "canvas": "^2.11.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "ispinner.css": "^3.1.1",
    "jsdom": "^24.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.400.0",
    "mustache": "^4.2.0",
    "mutation-observer": "^1.0.3",
    "next": "^14.2.4",
    "nth-check": "^2.1.1",
    "permessage-deflate": "^0.1.7",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "react-kasmvnc": "^2.2.0",
    "sharp": "^0.33.4",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.17.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@tsconfig/node12": "^12.1.3",
    "@types/async-lock": "^1.4.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/ws": "^8.5.10",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.16.1",
    "rimraf": "^5.0.7",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.16.2",
    "turbo": "^2.0.6",
    "typescript": "^5.5.3",
    "v0": "^2.2.3"
  }
}
