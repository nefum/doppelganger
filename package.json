{
  "name": "doppelganger-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "init": "git submodule update --init --recursive",
    "android:fetch": "bash scripts/download-scrcpy-server.sh & find scripts -name 'download-pheidippides-*.sh' -type f | xargs -I {} -P 0 bash {}",
    "db:generate": "prisma generate",
    "db:migrate": "dotenvx run --env-file=.env.local -- prisma migrate dev",
    "db:seed": "dotenvx run --env-file=.env.local -- tsx prisma/seed.ts",
    "server:build": "rimraf -rf dist/ && babel --config-file \"./server/babel.config.json\" --copy-files -x \".ts\" server/src -d dist && sentry-cli sourcemaps inject dist && sentry-cli sourcemaps upload dist",
    "server:dev": "tsx server/src/index.ts",
    "workers:dev": "rimraf -rf public/workers/ && webpack --config workers/webpack.config.cjs",
    "workers:build": "rimraf -rf public/workers/ && webpack --config workers/webpack.config.cjs --mode production",
    "licenses:check": "rimraf \"src/app/(userland)/(contained)/licenses/licenses.json\" && license-checker --production --json --out \"src/app/(userland)/(contained)/licenses/licenses.json\"",
    "dev": "nodemon -x \"turbo workers:dev && pnpm run server:dev\"",
    "build": "next build",
    "start": "NODE_ENV=production NODE_OPTIONS=\"--import ./dist/instrument.js\" node dist/index.js",
    "lint": "next lint --max-warnings 0",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "format": "prettier --write \"**/*.{json,json5,ts,tsx,md,mdx,js,jsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "ui:add": "pnpx shadcn-ui@latest add"
  },
  "type": "commonjs",
  "engines": {
    "node": ">=22.4.0"
